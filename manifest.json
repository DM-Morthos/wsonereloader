{
  "manifest_version": 2,
  "name": "Workspace One Reloader",
  "version": "0.1",
  "description": "Reloads the Workspace One Console before the session expires.",
  "permissions": ["tabs", "idle", "activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
